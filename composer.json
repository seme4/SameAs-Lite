{
    "name": "seme4/sameas-lite",
    "version": "0.0.1",
    "description": "SameAs Lite is a framework for storing and managing SameAs relationships between Linked Data and Semantic Web resources",
    "type": "library",
    "keywords": [ "SameAs", "Linked Data", "RDF", "Semantic Web" ],
    "license": "MIT Public License",
    "authors": [
        {
            "name": "Seme4 Ltd",
            "email": "sameAs@sseme4.com",
            "homepage": "http://www.seme4.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "easyrdf/easyrdf": "*",
        "ml/json-ld": "~1.0",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-mysql" : "*",
        "ext-pdo_sqlite" : "*",
        "ptlis/conneg": "*",
        "slim/slim": "*",
        "slim/views": "*",
        "twig/twig": "*",
        "twbs/bootstrap": "3.*",
        "afarkas/html5shiv": "*",
        "rogeriopradoj/respond": "*",
        "components/jquery": "1.*",
        "whitehat101/apr1-md5": "~1.0"
    },
    "require-dev": {
        "php": ">=5.4.0",
        "ext-xsl": "*",
        "phpdocumentor/phpdocumentor": "*",
        "squizlabs/php_codesniffer": "2.*",
        "fabpot/php-cs-fixer": "*",
        "phploc/phploc": "*",
        "phpunit/PHPUnit": "*",
        "behat/behat": "*"
    },

    "autoload": {
        "psr-4": {
            "SameAsLite\\Tests\\": "tests/phpUnit/",
            "SameAsLite\\": "src/"
        }
    }
}
